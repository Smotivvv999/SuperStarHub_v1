local function SafeLoad(url)
    local success, result = pcall(function()
        local raw = game:HttpGet(url)
        local func = loadstring(raw)
        return func
    end)

    if success and result then
        result() -- Jalankan script
    else
        warn("Gagal menjalankan script dari URL:", result)
    end
end

SafeLoad("https://raw.githubusercontent.com/Smotivvv999/SuperStarHub_v1/main/load.lua")

